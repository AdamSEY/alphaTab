package net.coderline.jsgs.model;

class Rectangle 
{
	public var X:Int;
	public var Y:Int;
	public var Width:Int;
	public var Height:Int;
	
	public function new(x:Int, y:Int, width:Int, height:Int) 
	{
		this.X = x;
		this.Y = y;
		this.Width = width;
		this.Height = height;
	}
	
}