{
  "output": "Haxe\\src-gen",
  "processorCount": 1,
  "postbuild": [
    {
      "name": "Building JavaScript (debug)",
      "executable": "haxe",
      "arguments": "test.hxml",
      "workingDirectory": "Haxe"
    },
    {
      "name": "Updating unit tests",
      "executable": "haxelib",
      "arguments": "run munit gen",
      "workingDirectory": "Haxe"
    },
    {
      "name": "Copying output to runner directory",
      "executable": "cmd.exe",
      "arguments": "/C copy bin\\js_test.js report\\test-runner\\js_test.js",
      "workingDirectory": "Haxe"
    },
    {
      "name": "Copying output to runner directory",
      "executable": "cmd.exe",
      "arguments": "/C xcopy ..\\AlphaTab.Test\\TestFiles Haxe\\report\\test-runner\\TestFiles\\ /Y /S /D"
    }
  ]
}